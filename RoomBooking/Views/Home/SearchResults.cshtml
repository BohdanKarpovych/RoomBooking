@model System.Collections.Generic.List<RoomBooking.Models.Room>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <p>Search results</p>
    <table class="mainTables">
        <tr class="mainTables">
            <th class="mainTables" colspan="2"><p>Room list</p></th>
        </tr>
        @foreach (var item in Model)
            {
            <tr class="mainTables">
                <td class="mainTables"><p>@item.RoomNumber</p></td>
                <td class="mainTables"><p><a href="/Home/Book/@item.RoomId">Book</a></p></td>
            </tr>
        }
    </table>
</div>
